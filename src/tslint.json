{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "quotemark": [
      false,
      "single"
    ],
    "no-submodule-imports": false,
    "semicolon": [
      false,
      "always"
    ],
    "jsx-no-multiline-js": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "object-literal-key-quotes": false,
    "jsx-wrap-multiline": false,
    "jsx-alignment": false
  }
}