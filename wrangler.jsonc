{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "phantomtype",
  "compatibility_date": "2025-08-03",
  "main": "./src/index.tsx",
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "vars": {
    "PUBLIC_HOST": "https://phantomtype.com"
  },
  "routes": [{ "pattern": "phantomtype.com", "custom_domain": true }]
}
