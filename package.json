{
  "name": "phantomtype.com",
  "version": "1.0.0",
  "description": "phantomtype.com",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/YusukeKokubo/bibonote"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "preserve": "npm run && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "clean": "rimraf \"dist/functions\" && rimraf \"out\"",
    "build-app": "next build",
    "build-functions": "tsc --project functions",
    "lint-app": "tslint --project src",
    "typecheck-app": "tsc --project src",
    "lint-functions": "tslint --project functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "app-export": "npm run build-app && next export",
    "app-deploy": "now --prod",
    "func-deploy": "npm run build-functions && firebase deploy --only functions"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@google-cloud/storage": "^5.3.0",
    "@types/js-cookie": "^2.2.6",
    "autoprefixer": "^9.8.6",
    "date-fns": "^2.16.1",
    "date-fns-timezone": "^0.1.4",
    "exif-reader": "^1.0.3",
    "firebase": "^7.19.1",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "fraction.js": "^4.0.12",
    "js-cookie": "^2.2.1",
    "next": "^9.5.2",
    "postcss-import": "^12.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-lazy-load-image-component": "^1.5.0",
    "sharp": "^0.26.0",
    "tailwindcss": "^1.7.6"
  },
  "devDependencies": {
    "@types/node": "14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/sharp": "^0.25.1",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "firebase-tools": "^8.9.2",
    "rimraf": "3.0.2",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.2"
  }
}
