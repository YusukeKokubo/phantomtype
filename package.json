{
  "name": "phantomtype.com",
  "version": "1.0.0",
  "description": "phantomtype.com",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "preserve": "npm run && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"out\"",
    "build-app": "next build",
    "build-functions": "tsc --project functions",
    "lint-app": "tslint --project src",
    "typecheck-app": "tsc --project src",
    "lint-functions": "tslint --project functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "app-export": "npm run build-app && next export",
    "app-deploy": "firebase deploy --only hosting"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.2.0",
    "date-fns": "^2.9.0",
    "date-fns-timezone": "^0.1.4",
    "exif-reader": "^1.0.3",
    "firebase": "^7.6.2",
    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.1.0",
    "fraction.js": "^4.0.12",
    "next": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebase-hooks": "^2.1.1",
    "react-lazy-load-image-component": "^1.4.1",
    "sharp": "^0.23.4"
  },
  "devDependencies": {
    "@types/node": "12.0.12",
    "@types/react": "^16.6.0",
    "@types/react-dom": "16.8.4",
    "@types/react-lazy-load-image-component": "^1.3.0",
    "@types/sharp": "^0.23.1",
    "cpx": "1.5.0",
    "cross-env": "5.2.0",
    "firebase-tools": "^7.1.0",
    "rimraf": "2.6.2",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.7.4"
  }
}