{
  "name": "phantomtype.com",
  "version": "1.0.0",
  "description": "phantomtype.com",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/YusukeKokubo/bibonote"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next",
    "preserve": "npm run && npm run build-functions && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "clean": "rimraf \"dist/functions\" && rimraf \"out\"",
    "build-app": "next build",
    "build-functions": "tsc --project functions",
    "lint-app": "tslint --project src",
    "typecheck-app": "tsc --project src",
    "lint-functions": "tslint --project functions",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "app-export": "npm run build-app && next export",
    "app-deploy": "now --prod",
    "func-deploy": "npm run build-functions && firebase deploy --only functions"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.0.0",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@types/js-cookie": "^2.2.6",
    "babel-plugin-styled-components": "^1.10.7",
    "date-fns": "^2.13.0",
    "date-fns-timezone": "^0.1.4",
    "exif-reader": "^1.0.3",
    "firebase": "^7.14.3",
    "firebase-admin": "^8.12.1",
    "firebase-functions": "^3.6.1",
    "fraction.js": "^4.0.12",
    "js-cookie": "^2.2.1",
    "next": "^9.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.1.1",
    "react-lazy-load-image-component": "^1.4.3",
    "sharp": "^0.25.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-lazy-load-image-component": "^1.3.0",
    "@types/sharp": "^0.25.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "firebase-tools": "^8.2.0",
    "rimraf": "3.0.2",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.2"
  }
}
